CREATE TABLE `paciente` (
  `pacid` int(11) NOT NULL AUTO_INCREMENT,
  `pacusuid` int(11) DEFAULT NULL,
  `paccod`  IDENTIDAD varchar(200) DEFAULT NULL,
  `pactok` TOKEN varchar(100) DEFAULT NULL,
  `pacfnm`  NOMBRES   varchar(140) DEFAULT NULL,
  `paclnm`  APELLIDOS varchar(140) DEFAULT NULL,
  `pacnac`  NACIMIENTO  DEFAULT NULL,
  `pacgen`  GENERO varchar(20) DEFAULT NULL,
  `pacdir`  DIRECCION text,
  `pacecv`  ESTAD CIVIL varchar(20) DEFAULT NULL,
  `pacocu`  OCUPACION varchar(20) DEFAULT NULL,
  `pacafm`  ANTECENDENTE DE LA MADREtext,
  `pacafp`  ANTECEDENTE DEL PADRE text,
  `pacafh`  ANTECEDENTEDES DEL HIJO text,
  `pacafo`  ANTECEDENTE OTRO text,
  `pacapp`  ANTECEDENTE PATOLOGICOS text,
  `pacanp`  ANTECEDENTES NO PATOLOGICOS text,
  `pacago`  ANTECEDENTES GINECOSTETRICOStext,
  `pacapr`  ANTECEDENTES PERINATALES text, 
  `pacale`  ALERGIAS text,
  `pacgps`  GRUPO SANGUINERO varchar(20) DEFAULT NULL,
  `paccor`   CORREO ELECTRONICO varchar(140) DEFAULT NULL,
  `pactlf`  TELEFONO  varchar(140) DEFAULT NULL,
  `pactl1`  TELEFONO 1 varchar(140) DEFAULT NULL,
  `pactl2`  TELEFONO 2 varchar(140) DEFAULT NULL,
  `pactl3`  TELEFONO 3 varchar(140) DEFAULT NULL,
  `pactip`  TIPO PACIENTE int(11) DEFAULT NULL,
  `pacava`  AVATAR IMAGEN varchar(140) DEFAULT NULL,
  `pacest`  ESTATU varchar(1) DEFAULT NULL,
  `pacefa`  EXAMEN FISICO  varchar(145) DEFAULT NULL,
  `pacefb` EXAMEN FISICO   varchar(145) DEFAULT NULL,
  `pacefc` EXAMEN FISICO   varchar(145) DEFAULT NULL,
  `pacefd` EXAMEN FISICO   varchar(145) DEFAULT NULL,
  `pacefe` EXAMEN FISICO   varchar(145) DEFAULT NULL,
  `paceff` EXAMEN FISICO   varchar(145) DEFAULT NULL,
  `pacefg` EXAMEN FISICO   varchar(145) DEFAULT NULL,
  `pacefh` EXAMEN FISICO   varchar(145) DEFAULT NULL,
  `pacefi` EXAMEN FISICO   varchar(145) DEFAULT NULL,
  `pacefj` EXAMEN FISICO   varchar(145) DEFAULT NULL,
  `pacefk` EXAMEN FISICO   varchar(145) DEFAULT NULL,  
  `pacobs` text,
  PRIMARY KEY (`pacid`)
) ENGINE=InnoDB AUTO_INCREMENT=0 DEFAULT CHARSET=latin1;